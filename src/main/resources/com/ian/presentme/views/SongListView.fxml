<?xml version="1.0" encoding="UTF-8"?>

<!--List of all songs-->
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="TOP_CENTER" spacing="5" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml">
        <Label styleClass="heading" text="Songs" />
        <TextField fx:id="songlist_search_field" promptText="Search..." />
        <ListView fx:id="songlist_listview" />
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<!--Currently active set list-->
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" alignment="TOP_CENTER" spacing="5">
    <Label fx:id="set_list_label" styleClass="heading" text="Set List" />
    <HBox spacing="5">
        <TextField fx:id="set_list_search_field" promptText="Search..." />
        <Button fx:id="set_list_create_button" text="New Set"/>
    </HBox>
    <ListView fx:id="set_list_listview" />
</VBox>
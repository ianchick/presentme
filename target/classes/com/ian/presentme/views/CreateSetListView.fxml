<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      prefHeight="100.0" prefWidth="200.0" spacing="10">
    <padding><Insets top="10" right="10" bottom="10" left="10"/></padding>

    <Label text="New Set Name"/>
    <TextField fx:id="create_set_view_title" promptText="Set List Name"/>
    <HBox spacing="10">
        <Button fx:id="create_set_view_save" text="Create"/>
        <Button fx:id="create_set_view_cancel" text="Cancel"/>
    </HBox>

</VBox>
